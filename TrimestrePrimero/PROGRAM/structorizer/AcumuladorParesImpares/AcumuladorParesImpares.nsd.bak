<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:nsd="https://structorizer.fisch.lu" version="3.29-13" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preThrow="throw" preForIn="foreach" stepFor="by" author="alumno" created="2019-10-04" changedby="alumno" changed="2019-10-07" origin="Structorizer 3.29-13" text="&#34;AcumuladorParesImpares&#34;" comment="" color="ffffff" type="program" style="nice">
	<children>
		<instruction text="&#34;const CONDICION_PARADA: int &#60;- 999&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;var pares: int &#60;- 0&#34;,&#34;var impares: int &#60;- 0&#34;,&#34;var total: int&#34;,&#34;var i: int &#60;- 0&#34;,&#34;var actual &#60;- CONDICION_PARADA&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
		<repeat text="&#34;actual != CONDICION_PARADA&#34;" comment="" color="ffffff" disabled="0">
			<qRepeat>
				<instruction text="&#34;OUTPUT &#34;&#34;&#62; Introduzca un nº o 999 para terminar: &#34;&#34;&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
				<instruction text="&#34;INPUT actual&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
				<alternative text="&#34;actual != CONDICION_PARADA&#34;" comment="" color="ffffff" disabled="0">
					<qTrue>
						<instruction text="&#34;i &#60;- i + 1&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
						<alternative text="&#34;actual % 2 == 0&#34;" comment="" color="ffffff" disabled="0">
							<qTrue>
								<instruction text="&#34;pares &#60;- pares + actual&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
							</qTrue>
							<qFalse>
								<instruction text="&#34;impares &#60;- impares + actual&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
							</qFalse>
						</alternative>
					</qTrue>
					<qFalse color="ffffff">
					</qFalse>
				</alternative>
			</qRepeat>
		</repeat>
		<instruction text="&#34;total &#60;- pares + impares&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;OUTPUT &#34;&#34;&#62; Números introducidos: &#34;&#34; + i&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;OUTPUT &#34;&#34;&#62; Pares: &#34;&#34; + pares + &#34;&#34;, Impares: &#34;&#34; + impares + &#34;&#34;, Total: &#34;&#34; + total&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
	</children>
</root>